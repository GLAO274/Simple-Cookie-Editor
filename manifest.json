{
  "manifest_version": 3,
  "name": "Simple Cookie Editor",
  "version": "1.0.0",
  "description": "View, edit, add, delete, and manage cookies for the current tab",
  "permissions": [
    "cookies",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png",
    "default_title": "Simple Cookie Editor"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
